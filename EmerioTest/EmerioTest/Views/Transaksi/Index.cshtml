@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Account ID</th>
        <th>Transaction Date</th>
        <th>Description</th>
        <th>Debit Card Status</th>
        <th>Ammount</th>
    </tr>
    @for (int i=0; i < Model.Rows.Count; i++)
    {
    <tr>
        <th>@Model.Rows[i][1]</th>
        <th>@Model.Rows[i][2]</th>
        <th>@Model.Rows[i][3]</th>
        <th>@Model.Rows[i][4]</th>
        <th>@Model.Rows[i][5]</th>
    </tr>
    }
</table>

<a href="@Url.Action("Create","Transaksi")">Tambah Transaksi</a>

