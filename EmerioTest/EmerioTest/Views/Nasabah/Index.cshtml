@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>ID Nasabah</th>
        <th>Nama Nasabah</th>
    </tr>
    @for (int i=0; i<Model.Rows.Count; i++)
    {
    <tr>
        <th>@Model.Rows[i][0]</th>
        <th>@Model.Rows[i][1]</th>
    </tr>
    }
</table>

<a href="@Url.Action("Create","Nasabah")">Nasabah Baru</a>